				<!-- <button type="submit">Add</button> -->
				<div class="form-group row">
					<label for="inputEmail3" class="col-sm-2 col-form-label">Name :</label>
					<div class="col-sm-10">
				  		<input type="text" required="true" name="name" class="form-control" id="inputEmail3">
					</div>
			  	</div>
			  	<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Roll No. :</label>
					<div class="col-sm-10">
				  		<input type="number" required="true" name="rno" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Hostel Name :</label>
					<div class="col-sm-10">
					  	<input type="text" required="true" name="hostelname" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Room No. :</label>
					<div class="col-sm-10">
						<input type="text" required="true" name="roomno" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Branch Name :</label>
					<div class="col-sm-10">
					  	<input type="text" name="branchname" required="true" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">E-Mail ID :</label>
					<div class="col-sm-10">
					  	<input type="email" name="emailid" required="true" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Phone No. :</label>
					<div class="col-sm-10">
					  	<input type="number" name="phoneno" required="true" class="form-control" id="inputPassword3">
					</div>
				</div>
				<div class="form-group row">
					<label for="inputPassword3" class="col-sm-2 col-form-label">Year Of Admission :</label>
					<div class="col-sm-10">
					  	<input type="number" name="year" required="true" class="form-control" id="inputPassword3">
					</div>
				</div>
				<button type="submit" class="btn btn-info">Add</button>

			</form>
			<%if(typeof error!='undefined'){%>
				<div class="alert alert-danger" role="alert">
					<p><%=error%></p>
				</div>
			<%}%>
			<%if(typeof success!='undefined'){%>
				<div class="alert alert-success" role="alert">
					<p><%=success%></p>
				</div>
			<%}%>
